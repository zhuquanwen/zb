<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iscas.zb.controller.LoginController">
   <children>
      <GridPane layoutX="29.0" layoutY="25.0" prefHeight="94.0" prefWidth="263.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="用户名：" />
            <Label text="密 码：" GridPane.rowIndex="1" />
            <TextField fx:id="usernameTextField" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="resetButton" defaultButton="true" layoutX="173.0" layoutY="116.0" mnemonicParsing="false" onAction="#processReset" text="重 置">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="loginButton" layoutX="79.0" layoutY="116.0" mnemonicParsing="false" onAction="#processLogin" text="登 陆">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ProgressIndicator fx:id="pi" layoutX="136.0" layoutY="148.0" prefHeight="33.0" prefWidth="40.0" />
   </children>
</AnchorPane>
